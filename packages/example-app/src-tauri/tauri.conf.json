{
  "productName": "inKibra Tauri Plugin Testbed",
  "version": "../package.json",
  "identifier": "com.inkibra.tauri-plugin-testbed",
  "build": {
    "frontendDist": "../dist",
    "beforeDevCommand": "npm run build",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "inKibra Tauri Plugin Testbed",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json"
}
